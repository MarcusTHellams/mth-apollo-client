{
  "usesTypeScript": false,
  "usesCssModule": true,
  "cssPreprocessor": "scss",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "customTemplates": {
        "component": "templates/component/component.js"
      },
      "path": "src/components",
      "withLazy": false,
      "withStory": false,
      "withStyle": false,
      "withTest": false
    },
    "page": {
      "customTemplates": {
        "component": "templates/page/page.js"
      },
      "path": "src/pages",
      "withLazy": false,
      "withStory": false,
      "withStyle": true,
      "withTest": false
    },
    "layout": {
        "path": "src/layout",
        "withLazy": false,
        "withStory": false,
        "withStyle": false,
        "withTest": false
      }
  }
}
